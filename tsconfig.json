{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "vue.d.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["/src/*"],
      "src/*": ["/src/*"]
    },
    "noImplicitAny": false
  },

  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
